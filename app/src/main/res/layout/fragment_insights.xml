<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="store.modules.insights.InsightsFragment">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <!--    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Coming soon"
            android:textSize="@dimen/_16ssp"
            android:fontFamily="@font/open_sans"
            android:textColor="@color/colorTextHint"/>-->

        <FrameLayout
            android:id="@+id/delivered_orderContainer"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
           >
        </FrameLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_6sdp"
            android:background="@color/colorDivider"
            android:layout_marginBottom="@dimen/_2sdp"/>
        <FrameLayout
            android:id="@+id/rejected_orderContainer"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            >
        </FrameLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:orientation="vertical">
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_6sdp"
            android:background="@color/colorDivider"
            android:visibility="gone"
            android:layout_marginBottom="@dimen/_2sdp"/>
        <FrameLayout
            android:id="@+id/preparation_timeContainer"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            >
        </FrameLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_6sdp"
            android:background="@color/colorDivider"
            android:layout_marginBottom="@dimen/_2sdp"/>
        <FrameLayout
            android:id="@+id/complaintsContainer"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            >
        </FrameLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_6sdp"
            android:background="@color/colorDivider"
            android:layout_marginBottom="@dimen/_2sdp"/>
        <FrameLayout
            android:id="@+id/top_dishesContainer"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            >
        </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</ScrollView>


</LinearLayout>